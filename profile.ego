export PERF_DATADIR=/opt/ibm/spectrumcomputing/perf/data
export EGO_LOCAL_CONFDIR=/opt/ibm/spectrumcomputing/kernel/conf
export PMR_VERSION=7.2
export PI_TOP=/opt/ibm/spectrumcomputing/perf

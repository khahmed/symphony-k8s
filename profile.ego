#!/bin/sh
SYM_TOP=/opt/ibm/spectrumcomputing
# Our scripts use uname -a to figure out the platform they are running on. Docker doesn't virtualize that so it reports the 
# host OS. If the host OS is Ubuntu and Sym container is CentOS scripts get confused
source $SYM_TOP/kernel/conf/profile.ego
ARCH=`arch`
export PERF_WORKDIR=/opt/ibm/spectrumcomputing/kernel/conf/../../perf/work
export MANPATH=/opt/ibm/spectrumcomputing/3.6/man:
export EGO_CONFDIR=/opt/ibm/spectrumcomputing/kernel/conf
export SOAM_BINDIR=/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/bin
export EGO_TOP=/opt/ibm/spectrumcomputing
export PERF_LIB_TYPE=linux-$ARCH
export MRSH_BUILD_NO=0
export EGO_VERSION=3.6
export PI_LOGDIR=/opt/ibm/spectrumcomputing/perf/logs
export EGO_SERVERDIR=/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/etc
export PI_CONFDIR=/opt/ibm/spectrumcomputing/kernel/conf/../../perf/conf
export OS_TYPE=linux-$ARCH
export SOAM_BINARY_TYPE=linux-$ARCH
export EGO_MACHINE_TYPE=linux-$ARCH
export PERF_ENV=-DPERF_TOP=/opt/ibm/spectrumcomputing/perf -DPERF_CONFDIR=/opt/ibm/spectrumcomputing/kernel/conf/../../perf/conf -DPERF_WORKDIR=/opt/ibm/spectrumcomputing/kernel/conf/../../perf/work -DPERF_LOGDIR=/opt/ibm/spectrumcomputing/perf/logs -DPERF_DATADIR=/opt/ibm/spectrumcomputing/perf/data
export LD_LIBRARY_PATH=/opt/ibm/spectrumcomputing/soam/mapreduce/7.2/linux-$ARCH/lib64:/opt/ibm/spectrumcomputing/soam/mapreduce/7.2/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/lib64:/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/integration/yarn/3.6/linux-$ARCH/lib/:/opt/ibm/spectrumcomputing/perf/ego/3.6/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/perf/soam/7.2/linux-$ARCH/lib:@SOAM_LIBDIR@:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/lib/jni
export PI_ROOT=/opt/ibm/spectrumcomputing/perf
export EGO_LIBDIR=/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/lib
export LIBPATH=/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/lib64:/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/integration/yarn/3.6/linux-$ARCH/lib/:/opt/ibm/spectrumcomputing/perf/ego/3.6/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/perf/soam/7.2/linux-$ARCH/lib:@SOAM_LIBDIR@:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/lib/jni
export BINARY_TYPE=linux-$ARCH
export EGOJRE_VERSION=8.0.3.21
export PERF_TOP=/opt/ibm/spectrumcomputing/perf
export PATH=/opt/ibm/spectrumcomputing/soam/mapreduce/7.2/linux-$ARCH/bin:/bin:/jre/bin:/usr/bin:/usr/local/bin:/local/bin:/sbin:/usr/sbin:/usr/ucb:/usr/bsd:/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/bin:/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/etc:/opt/ibm/spectrumcomputing/integration/yarn/3.6/bin:/opt/ibm/spectrumcomputing/perf/3.6/bin:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/etc:/opt/ibm/spectrumcomputing/3.6/linux-$ARCH/bin:/usr/local/sbin
export PMR_SERVERDIR=/opt/ibm/spectrumcomputing/soam/mapreduce/7.2/linux-$ARCH/etc
export SOAM_HOME=/opt/ibm/spectrumcomputing/soam
export PERF_LOGDIR=/opt/ibm/spectrumcomputing/perf/logs

export PERF_LIB=/opt/ibm/spectrumcomputing/perf/ego/3.6/linux-$ARCH/lib:/opt/ibm/spectrumcomputing/perf/soam/7.2/linux-$ARCH/lib
export PWD=/
export JAVA_HOME=
export PERF_ETCDIR=/opt/ibm/spectrumcomputing/perf/3.6/etc
export SOAM_SERVERDIR=/opt/ibm/spectrumcomputing/soam/7.2/linux-$ARCH/etc
export PERF_VERSION=3.6
export EGOYARN_HOME=/opt/ibm/spectrumcomputing/integration/yarn
export EGOYARN_SERVERDIR=/opt/ibm/spectrumcomputing/integration/yarn/3.6/etc
